{
  "expo": {
    "name": "Right Now Tracker",
    "slug": "rn-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFF7F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mhawryluk.rntracker"
    },
    "android": {
      "package": "mhawryluk.rntracker"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
